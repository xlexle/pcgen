<span>{{count}} hakutulosta</span>
<div class="vertical-space"></div>
<div class="row listings-area">
  <div class="col-xs-12">
  {{#if model}}
  {{#each model as |item index|}}
  <div class="product-container">

    <div class="row">
      {{#link-to 'admin.products.power-supplies.power-supply' item.id  data-toggle="modal" data-target="#product-details" tagName="div" classNames='col-xs-12'}}
        {{#products/tile-heading brand=item.propertyList.brand name=item.propertyList.productName sku=item.propertyList.sku}}
        <span class="sub-text">{{item.formFactor}}</span>
        <span class="sub-text">{{item.efficiency}}</span>
        {{/products/tile-heading}}
      {{/link-to}}
    </div>
  </div>
  {{/each}}
  {{/if}}
  </div>

  <div class="modal fade" id="product-details" tabindex="-1">
    {{outlet}}
  </div>
</div>
