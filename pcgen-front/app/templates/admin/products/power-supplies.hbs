<div class="modal" id="product-details" tabindex="-1">
  {{#admin/products/product-modal product=selectedItem}}
    <!-- Nest components here! -->
    {{selectedItem.propertyList.brand}} {{selectedItem.propertyList.productName}}
  {{/admin/products/product-modal}}
</div>

<span>{{count}} hakutulosta</span>
<div class="vertical-space"></div>
<div class="row listings-area">
  <div class="col-xs-12">
  {{#if model}}
  {{#each model as |item index|}}
  <div class="product-container">

    <div class="row">
      <div class="col-xs-12" data-toggle="modal" data-target="#product-details" {{action "setSelectedItem" item on="click"}}>
        {{#products/tile-heading brand=item.propertyList.brand name=item.propertyList.productName sku=item.propertyList.sku}}
        <span class="sub-text">{{item.formFactor}}</span>
        <span class="sub-text">{{item.efficiency}}</span>
        {{/products/tile-heading}}
      </div>
    </div>
  </div>
  {{/each}}
  {{/if}}
  </div>
</div>
